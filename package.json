{
  "name": "@neontribe/www",
  "description": "Neontribe website built with Gatsby",
  "version": "1.0.0",
  "dependencies": {
    "gatsby": "2.9.0",
    "gatsby-image": "2.1.3",
    "gatsby-plugin-manifest": "2.1.1",
    "gatsby-plugin-offline": "2.1.1",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-sharp": "2.1.3",
    "gatsby-source-filesystem": "2.0.39",
    "gatsby-transformer-sharp": "2.1.21",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "postcommit": "git update-index --again",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "yarn run develop",
    "start-prototype": "live-server prototype/",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "now-build": "npm run build"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.16.0",
    "eslint-config-react-app": "4.0.1",
    "eslint-plugin-flowtype": "3.10.1",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "2.4.1",
    "lint-staged": "8.2.0",
    "live-server": "1.2.1",
    "prettier": "1.18.2"
  },
  "engines": {
    "yarn": "1.12.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neontribe/www"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint",
      "git add"
    ]
  },
  "homepage": "https://github.com/neontribe/www#readme"
}
